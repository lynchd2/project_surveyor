<%= link_to "New Survey", new_survey_path %></br></br>

<% @surveys.each do |survey| %>
  <h2>Survey Name: <%= link_to survey.title, survey_path(survey) %><h2>
  <h3>Survey description: <%= survey.description %></br></h3>
  <%= link_to "Take Survey", survey_path(survey) %>
  <%= link_to "Edit Survey", edit_survey_path(survey)%>
  <%= link_to "Destroy Survey", survey_path(survey), method: :delete, data: {confirm: "Are you sure you want to delete the survey? All questions will be deleted."} %>
  </br></br></br>
<% end %>